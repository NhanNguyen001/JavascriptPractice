// Object.values and Object.entries
let obj = {
	username0: 'Santa',
	username1: 'Rudolf',
	username2: 'Mr.Grinch'
}

Object.keys(obj).forEach((key, index) => {
	console.log(key, obj[key])
})

Object.values(obj).forEach(value => {
	console.log(value);
})


Object.entries(obj).forEach(entrie => {
	console.log(entrie)
})